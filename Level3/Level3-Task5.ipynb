   "source": [
    "def fib(num):\n",
    "    a=0\n",
    "    b=1\n",
    "    for _ in range(num):\n",
    "        yield a\n",
    "        a,b=b,a+b\n",
    "fib_sequence = list(fib(5))\n",
    "print(fib_sequence)   "
   ]
